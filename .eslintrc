{
    "extends": "airbnb",
    "parserOptions": {
    "ecmaVersion": 6
    },
    "env": {
    "node": true,
    "browser": true,
    "es6": true
    },
    "rules": {
    "comma-dangle": ["error", "never"],
    "no-unused-vars": ["error", {"vars": "local", "args": "none"}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-boolean-value": [0],
    }

}
